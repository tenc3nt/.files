steam(){
case "$1" in
"0")
cat << X0
    :  :  :  :  :  
     :  :  :  :  : 
     :  :  :  :  : 
    :  :  :  :  :  
X0
;;
"1")
cat << X0
     :  :  :  :  : 
     :  :  :  :  : 
    :  :  :  :  :  
    :  :  :  :  :  
X0
;;
"2")
cat << X0
     :  :  :  :  : 
    :  :  :  :  :  
    :  :  :  :  :  
     :  :  :  :  : 
X0
;;
"3")
cat << X0
    :  :  :  :  :  
    :  :  :  :  :  
     :  :  :  :  : 
     :  :  :  :  : 
X0
;;
esac
}

hotcoffee() {
  steam 1
  coffee

  x=0
  while :; do
cat << X0
[A[A[A[A[A[A[A[A[A[A[A[A[A[A[A[A[A[A[A[A
X0
    steam $x
    coffee
    [ "$x" -le "2" ] && x=$((x+1)) || x=0
    sleep 0.25
  done
}

case "$1" in
  *--burger|burger)
    burger
  ;;
  *--vburger|vburger)
    vburger
  ;;
  *--poptart|poptart)
    poptart
  ;;
  *--rice|rice)
    rice
  ;;
  *--waffles|waffles)
    waffles
  ;;
  *--pancakes|pancakes)
    pancakes
  ;;
  *--pizza2|pizza2)
    pizza2
  ;;
	*--pizza|pizza)
    pizza
  ;;
	*--taco|taco)
		taco
	;;
  *--coffee|coffee)
    coffee
  ;;
  *--hotcoffee|hotcoffee)
    hotcoffee
  ;;
  *)
    taco
    usage
  ;;
esac
